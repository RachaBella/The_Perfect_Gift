<%= render partial: "/layouts/navbar" %>
The chosen recipient :
<%= @recipientChosen %>

The chosen occasion :
<%= @occasionChosen%>
<% count=0 %><br>
	
	<hr>
		
    <div class="container">
    <div class="row">
        <div class= "col-md-12 div" align="center">
            <div class ="col-xs-12 col-sm-12 col-md-6" >
               <%= collection_select(:user, :recipient_id , @recipients, :id, :name, {:prompt=> @recipientChosen}, {:class =>"form-control"})%>
            </div>
            <div class="col-xs-12 col-sm-12 col-md-4">
            <%= select_tag(:occasion, options_for_select([['Birthday','Birthday'],['Wedding','Wedding'],['Christmas','Christmas'],['Thanksgiving','Thanksgiving'],['New year','New Year'],['Baby Shower', 'Baby Shower'], ['Wedding Shower', 'Wedding Shower'], ['Hanukah','Hanukah'],['Ramadan','Ramadan'],['Anniversary', 'Anniversary']]), {:class =>"form-control" ,:id => 'occasion'}) %>
              
            </div>
        <br>   
        <hr>
        <div class="col-md-12 div">
            <div id="carousel-example-generic" class="carousel slide" data-ride="carousel">
                <div class="col-md-12 div" align="center">
                  <ul class="carousel-inner">
                    <li class="item active">
                          <img src="<%=@finalResponse[0][:image_url] %>" alt="First slide">
                      </li>
                      <% @finalResponse.drop(1).each do |res|%>
                      <li class="item">
                          <img src="<%= res[:image_url] %>" alt="Second slide">
                          <p style='display:none' id="img"><%= res[:image_url] %></p>
                          <p style='display:none' id="keyword"><%= res[:keyword] %></p>
                          <p style='display:none' id="url"><%= res[:affiliate_url] %></p>
                          <p style='display:none' id="price"><%= res[:price] %></p>
                          <p style='display:none' id="description"><%= res[:description] %></p>

                      </li>
                      <%end%>
                  </ul>
                  <br>
                  <br>
                  <ol class="carousel-indicators">
                <% @finalResponse.each do |res|%>
                    <li data-target="#carousel-example-generic" data-slide-to="#{count}" class="active"></li>
                    <% count+=1 %>
                <%end%>
                </ol>
                </div>
            </div>
                
                

            </div>
            
        </div>
    </div>
   
      
      
<div id="push">
</div>

