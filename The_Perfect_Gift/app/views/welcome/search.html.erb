<%= render partial: "/layouts/navbar" %>

<% count=0 %><br>
	
	<hr>
		
    <div class="container">
    <div class="row">
        <div class="col-md-12 div">
            <div id="carousel-example-generic" class="carousel slide" data-ride="carousel">
                <div class="col-md-12 div" align="center">
                  <ul class="carousel-inner">
                    <li class="item active">
                          <img src="<%=@finalResponse[0][:image_url] %>" alt="First slide">
                      </li>
                      <% @finalResponse.each do |res|%>
                      <li class="item">
                          <img src="<%= res[:image_url] %>" alt="Second slide">
                      </li>
                      <%end%>
                  </ul>
                  <br>
                  <br>
                  <ol class="carousel-indicators">
                <% @finalResponse.each do |res|%>
                    <li data-target="#carousel-example-generic" data-slide-to="#{count}" class="active"></li>
                    <% count+=1 %>
                <%end%>
                </ol>
                </div>
            </div>
                
                

            </div>
            
        </div>
    </div>
   
      
      
<div id="push">
</div>

<div class="container">
  <div class="row">
      <div class="col-xs-12 col-sm-6 col-md-6" align="center">
           <a class="btn-floating btn-large waves-effect waves-light red">
            <i class="material-icons">+</i>
           </a>
      </div>
      
      <div class="col-xs-12 col-sm-6 col-md-6" align="center">
           <a class="btn-floating btn-large waves-effect waves-light red">
            <i class="material-icons">+</i>
           </a>
      </div>
          
</div>
  

